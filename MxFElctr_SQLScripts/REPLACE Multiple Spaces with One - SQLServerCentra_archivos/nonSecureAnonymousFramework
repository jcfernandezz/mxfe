i.style.width="1px";
i.style.height="1px";
if(n.embedded){i.style.display="inline-block"
}if(!n.embedded){i.style.position="absolute";
i.style.left="-2345px";
i.style.top="0"
}if(n.closeOnDocClick){IN.Event.on(document,"click",function(){this.transport.destroy()
},this)
}},this);
if(!n.disableRefresh){IN.Event.on(IN,"refresh",function(){this.transport.refresh()
},this);
IN.Event.on(IN,"logout",function(){this.transport.refresh()
},this)
}return[c,b]
});


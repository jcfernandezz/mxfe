this.transportOptions.push("height="+this.height);
switch(this.mode){case"inline-iframe":case"iframe":case"embedded":case"modal":case"hovercard":case"invisible":this.transportOptions.push("mode=wrapper");
break;
case"popup":win=window.open(this.transportUrl+"#mode=popup-wait&target=easyXDM_"+this.channelId+"_provider","easyXDM_"+this.channelId+"_provider_popup","width="+this.width+", height="+this.height);
this.transportOptions.push("mode=popup");
break
}return this
}).Method("remove",function remove(){if(this.transport){this.transport.destroy();
this.transport=null
}this.onWindowRemove.fire();
this.Parent()
}).Method("params",function params(a){this.postParams=a;
return this
}).Method("ready",function ready(b,a){this.callbacks.ready.push({fn:b||function(){},scope:a||window});
return this
}).Method("success",function success(b,a){this.callbacks.success.push({fn:b||function(){},scope:a||window});
return this
}).Method("error",function error(b,a){this.callbacks.error.push({fn:b||function(){},scope:a||window});
return this
}).Method("handleWidgetReady",function handleWidgetReady(e,f,c){if(this.callbacks.ready.length){for(var d=0,b=this.callbacks.ready.length;
d<b;
d++){var a=this.callbacks.ready[d];
a.fn.call(a.scope,e)
}}f()
}).Method("handleWidgetSuccess",function handleWidgetSuccess(e,f,c){if(this.callbacks.success.length){for(var d=0,b=this.callbacks.success.length;
d<b;
d++){var a=this.callbacks.success[d];
a.fn.call(a.scope,e)
}}f()
}).Method("handleWidgetFailure",function handleWidgetFailure(e,f,c){if(this.callbacks.error.length){for(var d=0,b=this.callbacks.error.length;
d<b;
d++){var a=this.callbacks.error[d];
a.fn.call(a.scope,e)
}}f()
}).Method("place",function place(a,c){this.Parent(a,c);

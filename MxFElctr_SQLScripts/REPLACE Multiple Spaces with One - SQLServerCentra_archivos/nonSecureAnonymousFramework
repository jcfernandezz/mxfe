}t({width:s,height:p})
},login:function k(q,p){IN.GlobalEvents.refresh.fire()
},close:function m(q,p){d.transport.destroy()
},widgetSuccess:function l(q,r,p){d.handleWidgetSuccess(q,r,p)
},widgetError:function g(q,r,p){d.handleWidgetFailure(q,r,p)
},widgetReady:function j(q,r,p){d.handleWidgetReady(q,r,p)
}},remote:{mode:{},refresh:{},form:{}}};

e.transport.form({action:e.postUrl,items:e.postParams})
}};
a={local:{authorize:function d(p,m,o,q,n){IN.User.setAuthorized(p,m,o);
q()
},logout:function l(n,m){IN.User.setLoggedOut();
n()
},widgetSuccess:function k(n,o,m){e.handleWidgetSuccess(n,o,m)
},widgetError:function h(n,o,m){e.handleWidgetFailure(n,o,m)
},widgetReady:function j(n,o,m){e.handleWidgetReady(n,o,m)
}},remote:{form:{}}};

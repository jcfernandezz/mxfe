break;
case"hovercard":d=this.getEmbeddedRpc(b,{embedded:false,closeOnDocClick:true});
break;
case"invisible":d=this.getEmbeddedRpc(b,{embedded:false,closeOnDocClick:false,disableRefresh:true});
break;
case"modal":d=this.getEmbeddedRpc(b,{embedded:false});
break;
case"popup":d=this.getPopupRpc(b);
break
}this.transport=new IN.Lib.easyXDM.Rpc(d[0],d[1]);
return this
}).Method("getPopupRpc",function getPopupRpc(i){var e=this,b,a,f,c="";
